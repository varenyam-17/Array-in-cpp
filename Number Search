#include <iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of Array"<<endl;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cout<<"Enter element"<<endl;
        cin>>arr[i];
    }
    int s;
    cout<<"Enter number to be Searched"<<endl;
    cin>>s;
    for(int i=0;i<n;i++)
    {
        if(arr[i]==s)
        {
            cout<<"Present at position "<<i+1;
        }
    }
}
    
