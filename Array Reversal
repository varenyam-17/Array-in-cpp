#include <iostream>
using namespace std;
int main()
{
    int n;
    cout<<"Enter the size of Array"<<endl;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cout<<"Enter element"<<endl;
        cin>>arr[i];
    }
    cout<<"Resultant Array"<<endl;
    for(int i=n-1;i>=0;i--)
    {
        cout<<" "<<arr[i] ;
        
    }
    
    
}
